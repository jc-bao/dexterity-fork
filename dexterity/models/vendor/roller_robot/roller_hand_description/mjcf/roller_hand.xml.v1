<mujoco>
  <compiler angle="radian" coordinate="local" meshdir="../meshes" texturedir="../textures" />

  <visual>
    <quality shadowsize="4096" />
  </visual>

  <size njmax="500" nconmax="200" />

  <asset>
    <mesh name="roller" file="roller.stl" scale="1 1 1" />
    <mesh name="cube" file="cube.stl" scale="1 1 1" />
    <mesh name="cube2" file="cube2.stl" scale="1 1 1" />
    <mesh name="rectangle" file="rectangle.stl" scale="1 1 1" />
    <mesh name="rectangle2" file="rectangle2.stl" scale="1 1 1" />
    <texture builtin="none" name="cube_tex" height="32" width="32" rgb1="1 1 1" type="cube" file="wood.png"></texture>
    <material name="cube" texture="cube_tex" />
    <texture builtin="none" name="rubik_tex" height="32" width="32" rgb1="1 1 1" type="cube" file="rubik.png"></texture>
    <material name="rubik" texture="rubik_tex" />
  </asset>

  <default>
    <mesh scale="0.001 0.001 0.001" />
    <joint limited="true" damping="0.1" armature="0.005" margin="0.0" frictionloss="0.1" />
    <general ctrllimited="true" ctrlrange="-1 1" />
    <default class="visual">
      <geom contype="0" conaffinity="0" group="0" />
    </default>
    <default class="collision">
      <geom group="3" />
    </default>
  </default>

  <worldbody>
    <body name="forearm" pos="0 0 0.026625">
      <geom mass="0.1" pos="0 0 0" rgba="0.855 0.267 0.314 1" size="0.0375 0.026625" type="cylinder" />
      <!-- Left -->
      <body name="BodyLF" pos="0.0 -0.0262 0.073525">
        <joint name="JointLF" axis="0 1 0" type="slide" range="0 0.1" damping="100" armature='5' />
        <geom name="GeomLF" mass="0.01" size="0.005 0.005 0.03" rgba="0.063 0.522 0.737 1" type="box" />
        <body name="BodyLP" pos="0.0 0.0184 0.043525">
          <joint axis="0 1 0" damping="0.5" name="JointLP" pos="0.0 -0.0184 0" type="hinge" range="-1.57 1.57" />
          <geom name="GeLP" mass="0.01" size="0.005 0.0184 0.005" rgba="0.063 0.522 0.737 1" type="box" />
          <body name="BodyLR" pos="0.0 0.0184 0.0">
            <joint axis="0 0 1" damping="0.1" range="-1.57 1.57" name="JointLR" pos="0.0 0 0" type="hinge" />
            <geom name="GeomLR" mass="0.01" type="mesh" mesh="roller" rgba="0.973 0.741 0.498 0.7" />
          </body>
        </body>
      </body>
      <!-- Right -->
      <body name="BodyRF" pos="0.0 0.0262 0.073525">
        <joint name="JointRF" axis="0 1 0" type="slide" range="0 0.1" damping="100" armature='5' />
        <geom name="GeomRF" mass="0.01" size="0.005 0.005 0.03" rgba="0.063 0.522 0.737 1" type="box" />
        <body name="BodyRP" pos="0.0 -0.0184 0.043525">
          <joint axis="0 1 0" damping="0.5" name="JointRP" pos="0.0 -0.0184 0" type="hinge" range="-1.57 1.57" />
          <geom name="GeomRP" mass="0.01" size="0.005 0.0184 0.005" rgba="0.063 0.522 0.737 1" type="box" />
          <body name="BodyRR" pos="0.0 0.0184 0.0">
            <joint axis="0 0 1" damping="0.1" range="-1.57 1.57" name="JointRR" pos="0.0 0 0" type="hinge" />
            <geom name="GeomRR" mass="0.01" type="mesh" mesh="roller" rgba="0.973 0.741 0.498 0.7" />
          </body>
        </body>
      </body>
    </body>

  </worldbody>

  <actuator>
    <position name="ActLF" joint="JointLF" kp="20" />
    <position name="ACtLP" joint="JointLP" kp="500" />
    <position name="ACtLR" joint="JointLR" kp="500" />

    <position name="ActRF" joint="JointRF" kp="20" />
    <position name="ActRP" joint="JointRP" kp="500" />
    <position name="ActRR" joint="JointRR" kp="500" />
  </actuator>

  <contact>
    <exclude body1="BodyLF" body2="BodyLP" />
    <exclude body1="BodyLP" body2="BodyLR" />
    <exclude body1="BodyRF" body2="BodyRP" />
    <exclude body1="BodyRP" body2="BodyRR" />
  </contact>

</mujoco>