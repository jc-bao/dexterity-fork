        <joint name="LinkLF" axis="0 1 0" type="slide" range="0 0.1" damping="100" armature='5'/>
          <body name="link3" pos="0.0 0.0184 0.043525">
            <joint axis="0 1 0" damping="0.5" name="j3" pos="0.0 -0.0184 0" type="hinge" range="-1.57 1.57" />
