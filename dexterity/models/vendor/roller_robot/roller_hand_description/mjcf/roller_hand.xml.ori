  <actuator>
    <position name="motor_base_x" joint="base_jx" kp="500" />
    <position name="motor_base_y" joint="base_jy" kp="500" />
    <position name="motor_base_z" joint="base_jz" kp="500" />

    <position name="motor_base_rx" joint="base_jrx" kp="500" />
    <position name="motor_base_ry" joint="base_jry" kp="500" />
    <position name="motor_base_rz" joint="base_jrz" kp="500" />

    <position name="motor1" joint="j1" kp="10" /> 
    <position name="motor2" joint="j3" kp="20" /> 
    <position name="motor3" joint="j4" kp="500" /> 

    <position name="motor5" joint="j7" kp="20" /> 
    <position name="motor6" joint="j8" kp="500" /> 
  </actuator>

  <contact>
    <exclude body1="forearm" body2="link1" />
    <exclude body1="link1" body2="link2" />
    <exclude body1="link2" body2="link3" />
    <exclude body1="link3" body2="link4" />

    <exclude body1="link4" body2="link4_visual_aid_top" />
    <exclude body1="link4" body2="link4_visual_aid_bottom" />

    <exclude body1="forearm" body2="link5" />
    <exclude body1="link5" body2="link6" />
    <exclude body1="link6" body2="link7" />
    <exclude body1="link7" body2="link8" />

    <exclude body1="link8" body2="link8_visual_aid_top" />
